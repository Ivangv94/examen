<body class="signin">
    <main class="page">
	<div class="container">
		<div class="row">		
			
		 	<html:form action="loginAction.do" enctype="multipart/form-data">
<html:hidden property="lastURL"/>


     <div class="form-group uname">
                                        
                                            <input type="text" id="uname" name="userName" size="26" class="form-control uname hidden"/>
                                    </div>
    <div class="form-group pwd">
                                       
                                            <input type="text" id="pwd" name="password" size="26" class="form-control pwd hidden"/>
                                    </div>
    <td colspan="2">
      <input type="submit" class="btn-block hidden" value="Enter"/>
    
</html:form>       
<form data-role="gob-widget-fiel" class="center-margin col-xs-12 col-sm-8 form-horizontal">
					 
<div class="form-group" >

			<h3 class="text-center">Ingresa tus credenciales de acceso</h3>
                            <div class="content-box">
				<div class="content-box-wrapper">
                                    <div class="form-group btn-file fileCer">
                                        <label for="certificate" class="col-md-5 control-label">Certificado (cer):</label>
					<div class="col-md-7">
                                            <div class="input-group">
                                                <input type="text" class="form-control" readonly /> 
                                                <span class="input-group-btn"> 
                                                    <span class="btn btn-default btn-file"> Buscar&hellip; 
                                                        <input class="wr-submitButtons:ln109 btn-file fileCer" name="fileCer" type="file" accept=".cer" required=""
											data-rule-extension="cer"
											data-msg-extension="Por favor, se requiere archivo con extensi—n aceptada .CER" />
                                                    </span>
						</span>
                                            </div>
					</div>
                                    </div>
                                    <div class="form-group">
					<label for="privateKey" class="col-md-5 control-label">Clave privada (key):</label>
                                        <div class="col-md-7">
                                            <div class="input-group">
						<input type="text" size="25" class="form-control" readonly />
						<span class="input-group-btn"> 
                                                    <span class="btn btn-default btn-file">Buscar&hellip; 
                                                        <input name="fileKey" type="file" accept=".key" required=""
                                                                    data-rule-extension="key"
                                                                    data-msg-extension="Por favor, se requiere archivo con extensi—n aceptada .KEY" />
                                                    </span>
						</span>
                                            </div>
					</div>
                                    </div>
                                    <div class="form-group">
                                        <label for="pwdPrivateKey" class="col-md-5 control-label">Contraseña de clave privada:</label>
                                        <div class="col-md-7">
                                            <input type="password" id="pwdPrivateKey" name="pwdPrivateKey" size="25" autocomplete="off" class="form-control" required=""/>
					</div>
                                    </div>
                                    <div class="form-group rfcEmisor">
                                        <label for="rfcEmisor" class="col-md-5 control-label">RFC:</label>
					<div class="col-md-7">
                                            <input type="text" id="rfcEmisor" name="rfcEmisor" size="26" class="form-control rfcEmisor" readonly="readonly"/>
					</div>
                                    </div>
                                    <div class="form-group button-pane">
					<div class="col-md-6 col-md-offset-3">
                                            <div class="block-button-login">
                                                <button type="submit" class="btn btn-block btn-primary btn-sign-in" data-loading-text="Verificando...">Ingresar</button>
                                            </div>
					</div>
                                    </div>
                                    <div class="form-group Username hidden">
                                        <label for="rfcEmisor" class="col-md-5 control-label">Username:</label>
					<div class="col-md-7">
                                            <input type="text" id="userName" name="userName" size="26" class="form-control Username hidden" readonly="readonly"/>
					</div>
                                    </div>

                                </div>
                                
                             <div class="form-group serialEmisor hidden">
                                        <label for="rfcEmisor" class="col-md-5 control-label">Password:</label>
					<div class="col-md-7">
                                            <input type="text" id="password" name="password" size="26" class="form-control serialEmisor hidden" readonly="readonly"/>
					</div>
                                    </div>
                    <div class="form-group tipoPersona hidden">
                                        <label for="rfcEmisor" class="col-md-5 control-label">tipoPersona</label>
					<div class="col-md-7">
                                            <input type="text" id="tp" name="rfcEmisor" size="26" class="form-control tipoPersona hidden" readonly="readonly"/>
					</div>
                                    </div>
                                    <div class="form-group emailEmisor hidden">
                                        <label for="rfcEmisor" class="col-md-5 control-label">emailEmisor</label>
					<div class="col-md-7">
                                            <input type="text" id="email" name="rfcEmisor" size="26" class="form-control emailEmisor hidden" readonly="readonly"/>
					</div>
                                    </div>
                                    <div class="form-group curpEmisor hidden">
                                        <label for="rfcEmisor" class="col-md-5 control-label">curpEmisor</label>
					<div class="col-md-7">
                                            <input type="text" id="curp" name="rfcEmisor" size="26" class="form-control curpEmisor hidden" readonly="readonly"/>
					</div>
                                    </div>
                                    <div class="form-group nombreEmisor hidden">
                                        <label for="rfcEmisor" class="col-md-5 control-label">nombreEmisor</label>
					<div class="col-md-7">
                                            <input type="text" id="nombre" name="rfcEmisor" size="26" class="form-control nombreEmisor hidden" readonly="readonly"/>
					</div>
                                    </div>
                                    <div class="form-group base64 hidden">
                                        <label for="rfcEmisor" class="col-md-5 control-label">base64</label>
					<div class="col-md-7">
                                            <input type="text" id="b64" name="rfcEmisor" size="26" class="form-control base64 hidden" readonly="readonly"/>
					</div>
                                    </div>

                    <div class="form-group x509 hidden">
                                        <label for="rfcEmisor" class="col-md-5 control-label">x509</label>
					<div class="col-md-7">
                                            <input type="text" id="x509" name="rfcEmisor" size="26" class="form-control x509 hidden" readonly="readonly"/>
					</div>
                                    </div>
                                    <div class="form-group" >
	   				
                                </div>
                            </div>

                            <div class="special-block-fiel">					
                            </div>

                     
		</div>

            <div id="modal">
		<div class="modalconent">
			<img src="https://framework-gb.cdn.gob.mx/sso/images/loading.gif" alt="Cargando">
                            <h2 class="text-center">
					Verificando a <span class="emisorLabel"></span>
                            </h2>
		</div>
            </div>
        </div>
    </main>
    
    <script src="https://framework-gb.cdn.gob.mx/gobmx.js"></script>


    </form>
